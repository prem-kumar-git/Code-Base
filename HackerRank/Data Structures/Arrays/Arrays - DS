import java.io.*;
import java.util.*;
public class Solution {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n = scan.nextInt();
        int[] intArray = new int[n];
        for(int i=0; i<n; i++){
            intArray[i] = scan.nextInt();
        }
        for(int i=n-1; i>=0; i--){
            System.out.print(intArray[i] + " ");
        }
    }
}
